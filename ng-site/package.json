{
  "name": "ng-site",
  "version": "0.0.0",
  "scripts": {
    "ng": "ng",
    "start": "ng serve",
    "build": "ng build",
    "build:prod": "npm run route-list && ng build --prod",
    "test": "ng test",
    "lint": "ng lint",
    "e2e": "ng e2e",
    "dev:ssr": "ng run ng-site:serve-ssr",
    "serve:ssr": "node dist/ng-site/server/main.js",
    "build:ssr": "ng build --prod && ng run ng-site:server:production",
    "prerender": "npm run route-list && ng run ng-site:prerender",
    "route-list": "ts-node --compiler-options \"{\\\"module\\\": \\\"commonjs\\\", \\\"target\\\": \\\"ES2017\\\"}\" src/prerender.ts"
  },
