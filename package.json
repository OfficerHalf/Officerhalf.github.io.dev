{
  "name": "dev-site",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "buttercms": "^1.2.2",
    "emotion-theming": "^10.0.27",
    "normalize.css": "^8.0.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-helmet": "^6.0.0",
    "react-router": "^6.0.0-alpha.5",
    "react-router-dom": "^6.0.0-alpha.5",
    "react-scripts": "3.4.1",
    "typescript": "^3.7.5",
    "typography": "^0.16.19",
    "typography-plugin-code": "^0.16.19",
    "typography-theme-noriega": "^0.16.19"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "postbuild": "react-snap",
    "test": "react-scripts test",
    "eject": "react-scripts eject",
    "predeploy": "npm run build",
    "deploy": "gh-pages -d build -b master",
    "swap-cname": "rm ./build/CNAME && mv ./build/ProdCNAME ./build/CNAME",
    "preproduction": "npm run build && npm run swap-cname",
    "production": "gh-pages -d build -b master -r https://github.com/OfficerHalf/OfficerHalf.github.io.git"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/jest-dom": "^4.2.4",
    "@testing-library/react": "^9.5.0",
    "@testing-library/user-event": "^7.2.1",
    "@types/jest": "^24.9.1",
    "@types/node": "^12.12.38",
    "@types/react": "^16.9.34",
    "@types/react-dom": "^16.9.7",
    "@types/react-helmet": "^5.0.15",
    "@types/typography": "^0.16.3",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.3",
    "gh-pages": "^2.2.0",
    "prettier": "^2.0.5",
    "react-snap": "^1.23.0",
    "react-typography": "^0.16.19"
  }
